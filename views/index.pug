extends layout.pug

block content

  article.portfolio-intro

    
    h1 My Work
    p Here are a few of the projects that I've created so far!
  article.portfolio-project-thumbs
    .projects-box.box.grid
      each project in projects  
        .cell
          a(href=`/projects/${project.id}`)
            img.thumbnail(src=`${project.images_thumbnail}`, alt= `Thumbnail of ${project.project_name} Project`)
            h5= project.project_name

